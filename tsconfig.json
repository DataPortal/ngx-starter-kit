{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@coord-angular/*": [
        "libs/*"
      ],
      "@default/*": [
        "apps/webapp/src/app/*"
      ],
      "@env/*": [
        "apps/webapp/src/environments/*"
      ],
      "@coord-angular/navigator": [
        "libs/navigator/src/index.ts"
      ],
      "@coord-angular/socketio-plugin": [
        "libs/socketio-plugin/src/index.ts"
      ],
      "@coord-angular/tree": [
        "libs/tree/src/index.ts"
      ],
      "@coord-angular/home": [
        "libs/home/src/index.ts"
      ],
      "@coord-angular/dashboard": [
        "libs/dashboard/src/index.ts"
      ],
      "@coord-angular/not-found": [
        "libs/not-found/src/index.ts"
      ],
      "@coord-angular/experiments": [
        "libs/experiments/src/index.ts"
      ],
      "@coord-angular/widgets": [
        "libs/widgets/src/index.ts"
      ],
      "@coord-angular/material": [
        "libs/material/src/index.ts"
      ],
      "@coord-angular/animations": [
        "libs/animations/src/index.ts"
      ],
      "@coord-angular/utils": [
        "libs/utils/src/index.ts"
      ],
      "@coord-angular/auth": [
        "libs/auth/src/index.ts"
      ],
      "@coord-angular/shared": [
        "libs/shared/src/index.ts"
      ],
      "@coord-angular/app-confirm": [
        "libs/app-confirm/src/index.ts"
      ],
      "@coord-angular/draggable": [
        "libs/draggable/src/index.ts"
      ],
      "@coord-angular/breadcrumbs": [
        "libs/breadcrumbs/src/index.ts"
      ],
      "@coord-angular/scroll-to-top": [
        "libs/scroll-to-top/src/index.ts"
      ],
      "@coord-angular/scrollbar": [
        "libs/scrollbar/src/index.ts"
      ],
      "@coord-angular/context-menu": [
        "libs/context-menu/src/index.ts"
      ],
      "@coord-angular/theme-picker": [
        "libs/theme-picker/src/index.ts"
      ],
      "@coord-angular/quickpanel": [
        "libs/quickpanel/src/index.ts"
      ],
      "@coord-angular/ngx-pipes": [
        "libs/ngx-pipes/src/index.ts"
      ],
      "@coord-angular/svg-viewer": [
        "libs/svg-viewer/src/index.ts"
      ],
      "@coord-angular/led": [
        "libs/led/src/index.ts"
      ],
      "@coord-angular/chat-bot": [
        "libs/chat-bot/src/index.ts"
      ],
      "@coord-angular/json-diff": [
        "libs/json-diff/src/index.ts"
      ],
      "@coord-angular/toolbar": [
        "libs/toolbar/src/index.ts"
      ],
      "@coord-angular/sidenav": [
        "libs/sidenav/src/index.ts"
      ],
      "@coord-angular/loading-overlay": [
        "libs/loading-overlay/src/index.ts"
      ],
      "@coord-angular/grid": [
        "libs/grid/src/index.ts"
      ],
      "gridmap/*": [
        "dist/gridmap/*"
      ],
      "@coord-angular/gridmap": [
        "libs/gridmap/src/index.ts"
      ],
      "notifications/*": [
        "dist/notifications/*"
      ],
      "@coord-angular/notifications": [
        "libs/notifications/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
